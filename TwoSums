class Solution {
    func twoSum(_ nums: [Int], _ target: Int) -> [Int] {
        var result:[Int] = []
        
        for firstloop in 0...nums.count-1 {
    
            for secondloop in 0...nums.count-1 {
                if target == nums[firstloop] + nums[secondloop] {
                    if(firstloop != secondloop){
                        
                    return  [firstloop,secondloop]
                    }           
            
                }
            }
        }
        
        return []
    }
    
}

